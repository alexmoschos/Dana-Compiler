def main is int[10] : a as int
begin
	skip
	if 3 < 5:
	begin
		skip
	end
	loop a:
	begin
		if 1 < 2:
		begin 
			break : a
			continue: a
		end
	end
	def hello is byte: a as int, b as byte
	begin
		def world is byte: a as byte
		begin
			decl helloworld is byte[14]: a as byte[12]
		end
	end
	helloworld: 42, 15, "hello world"
